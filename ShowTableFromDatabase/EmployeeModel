using System.Linq;
using System.Web;

namespace SolutionProject.Models
{
    public class Employee
    {
        public int ID { get; set; }

        public string Name { get; set; }

        public string Surname { get; set; }

        public DateTime BirthDate { get; set; }
        
        public string City { get; set; }

    }
}
